@model List<HalloDoc_DAL.Models.Healthprofessional>;


<table  class="w-100 table table-hover overflow-scroll">
    <thead>
        <tr class="text-start">
            <th class="text-start ps-3">Profession</th>
            <th class="text-start ps-3">Business Name</th>
            <th class="text-start ps-3">Email</th>
            <th class="text-start ps-3">Fax Number</th>
            <th class="text-start ps-3">Phone Number</th>
            <th class="text-start ps-3">Business Contact</th>
            <th class="text-start ps-3" "">Actions</th>
        </tr>
    </thead>

    <tbody>

@foreach (var vendor in Model)
{
    <tr class="text-start data-row">

        <td class="text-black ps-3"> @ViewBag.ProfessionNames[(int)@vendor.Profession]</td>
        <td class="text-black ps-3">@vendor.Vendorname</td>
        <td class="text-black ps-3">@vendor.Email</td>
        <td class="text-black ps-3">@vendor.Faxnumber</td>
        <td class="text-black ps-3">@vendor.Phonenumber</td>
        <td class="text-black ps-3">@vendor.Businesscontact</td>
        <td class="text-black">
            <a href="/Admin/EditPartner?vendorid=@vendor.Vendorid" class="btn btn-sm hover_white btn-outline-info">Edit</a>
            <button class="btn btn-sm hover_white btn-outline-info open-modal"
                    data-vendorid="@vendor.Vendorid"
                    data-vendorname="@vendor.Vendorname">
                Delete
            </button>
        </td>
    </tr>
}
    </tbody>
</table>